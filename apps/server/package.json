{
  "name": "server",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "----DB": "",
    "introspect": "bun run drizzle-kit introspect:pg --config=drizzle.config.ts",
    "generate": "bun run drizzle-kit generate:pg --config=drizzle.config.ts",
    "migrate": "bun run src/db/migrate.ts",
    "studio": "bun run drizzle-kit studio",
    "----LINTER": "",
    "format": "bunx @biomejs/biome format --write .",
    "lint": "bunx @biomejs/biome lint --write .",
    "check": "bunx @biomejs/biome check --write .",
    "----TUNNEL": "",
    "tunnel": "bun run tunnel.ts",
    "----TEST": "",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "full-start": "bun migrate && bun start"
  },
  "dependencies": {
    "@better-auth/expo": "1.2.5-beta.3",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/static": "^1.2.0",
    "@elysiajs/swagger": "^0.8.5",
    "@ngrok/ngrok": "^1.4.1",
    "@react-email/components": "^0.0.34",
    "@types/jsdom": "^21.1.7",
    "axios": "^1.8.2",
    "better-auth": "^1.2.4",
    "config": "workspace:*",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.3",
    "drizzle-typebox": "^0.1.1",
    "drizzle-zod": "^0.5.1",
    "elysia": "^1.2.25",
    "jsdom": "^26.0.0",
    "openai": "^4.86.2",
    "pg": "^8.11.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "resend": "^4.2.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/jest": "^29.5.14",
    "@types/localtunnel": "^2.0.4",
    "@types/pg": "^8.11.0",
    "bun-types": "^1.1.10",
    "drizzle-kit": "^0.20.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2"
  },
  "module": "src/index.js"
}